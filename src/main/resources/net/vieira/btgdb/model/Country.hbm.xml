<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.vieira.btgbd.model.Country" table="country" proxy="net.vieira.btgbd.model.Country">
        <id name="id" type="long">
            <column name="id" />
        </id>
        <property name="country" type="string">
            <column name="country" />
        </property>
    </class>
    
</hibernate-mapping>

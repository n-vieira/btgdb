<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.vieira.btgbd.model.Institution" table="institution" proxy="net.vieira.btgbd.model.Institution">
        <id name="id" type="long">
            <column name="id" />
        </id>
        <property name="name" type="string">
            <column name="name" />
        </property>
        <property name="url" type="string">
            <column name="url" />
        </property>
        <property name="email" type="string">
            <column name="email" />
        </property>
        <property name="address" type="string">
            <column name="address" />
        </property>
        <many-to-one name="country" class="net.vieira.btgbd.model.Country">
            <column name="country_id" />
        </many-to-one>
    </class>
    
</hibernate-mapping>
